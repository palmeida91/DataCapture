          "rawSql": "WITH hourly_station_oee AS (\r\n    SELECT \r\n        date_trunc('hour', ct.time) as hour,\r\n        ct.sequence_id,\r\n        s.sequence_name,\r\n        AVG(ta.ta_percent) as avg_ta,\r\n        (PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY ct.desired_cycle_time_seconds)) /\r\n        NULLIF((PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY ct.cycle_time_seconds)), 0) * 100 as performance\r\n    FROM cycle_times ct\r\n    JOIN sequences s ON ct.sequence_id = s.sequence_id\r\n    JOIN technical_availability ta ON ct.sequence_id = ta.sequence_id\r\n        AND date_trunc('minute', ct.time) = date_trunc('minute', ta.time)\r\n    WHERE ct.time >= NOW() - INTERVAL '24 hours'\r\n        AND s.is_active = true\r\n    GROUP BY date_trunc('hour', ct.time), ct.sequence_id, s.sequence_name\r\n),\r\nhourly_bottleneck AS (\r\n    SELECT DISTINCT ON (hour)\r\n        hour,\r\n        sequence_name as bottleneck_name,\r\n        ROUND((avg_ta / 100.0 * performance / 100.0 * 100)::numeric, 1) as bottleneck_oee\r\n    FROM hourly_station_oee\r\n    ORDER BY hour, (avg_ta / 100.0 * performance / 100.0) ASC\r\n),\r\nhourly_quality AS (\r\n    SELECT \r\n        date_trunc('hour', time) as hour,\r\n        ROUND((MAX(good_parts)::float / NULLIF(MAX(good_parts) + MAX(reject_parts) + MAX(rework_parts), 0) * 100)::numeric, 1) as quality_percent\r\n    FROM quality_counters\r\n    WHERE time >= NOW() - INTERVAL '24 hours'\r\n    GROUP BY date_trunc('hour', time)\r\n)\r\nSELECT \r\n    hb.hour as time,\r\n    ROUND((hb.bottleneck_oee * hq.quality_percent / 100.0)::numeric, 1) as \"OEE %\",\r\n    hb.bottleneck_name as \"Bottleneck\"\r\nFROM hourly_bottleneck hb\r\nLEFT JOIN hourly_quality hq ON hb.hour = hq.hour\r\nORDER BY hb.hour;",
