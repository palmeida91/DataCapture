        {
          "dataset": "production",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH hour_boundaries AS (\r\n    -- Generate the two hours we want to show\r\n    SELECT date_trunc('hour', NOW()) as hour, 'Current Hour' as period\r\n    UNION ALL\r\n    SELECT date_trunc('hour', NOW() - INTERVAL '1 hour'), 'Previous Hour'\r\n),\r\nlatest_per_hour AS (\r\n    -- Get most recent data per hour\r\n    SELECT DISTINCT ON (date_trunc('hour', time))\r\n        date_trunc('hour', time) as hour,\r\n        good_parts,\r\n        reject_parts,\r\n        rework_parts\r\n    FROM quality_counters\r\n    WHERE time >= NOW() - INTERVAL '2 hours'\r\n    ORDER BY date_trunc('hour', time), time DESC\r\n)\r\nSELECT \r\n    hb.period as \"Period\",\r\n    COALESCE(lph.good_parts, 0) as \"Good\",\r\n    COALESCE(lph.rework_parts, 0) as \"Rework\",\r\n    COALESCE(lph.reject_parts, 0) as \"Reject\",\r\n    COALESCE(lph.good_parts + lph.reject_parts + lph.rework_parts, 0) as \"Total\"\r\nFROM hour_boundaries hb\r\nLEFT JOIN latest_per_hour lph ON hb.hour = lph.hour\r\nORDER BY hb.hour DESC;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
