          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH current_station_performance AS (\r\n    SELECT \r\n        s.sequence_name,\r\n        AVG(ta.ta_percent) as avg_ta,\r\n        PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY ct.desired_cycle_time_seconds) / \r\n        NULLIF(PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY ct.cycle_time_seconds), 0) * 100 as performance_percent\r\n    FROM cycle_times ct\r\n    JOIN sequences s ON ct.sequence_id = s.sequence_id\r\n    JOIN technical_availability ta ON ct.sequence_id = ta.sequence_id \r\n        AND date_trunc('minute', ct.time) = date_trunc('minute', ta.time)\r\n    WHERE ct.time >= date_trunc('hour', NOW())\r\n        AND s.is_active = true\r\n    GROUP BY s.sequence_id, s.sequence_name\r\n),\r\nbottleneck AS (\r\n    SELECT \r\n        sequence_name,\r\n        avg_ta / 100.0 * performance_percent / 100.0 * 100 as bottleneck_oee\r\n    FROM current_station_performance\r\n    ORDER BY (avg_ta / 100.0 * performance_percent / 100.0) ASC\r\n    LIMIT 1\r\n),\r\ncurrent_quality AS (\r\n    SELECT \r\n        good_parts::float / NULLIF(good_parts + reject_parts + rework_parts, 0) * 100 as quality_percent\r\n    FROM quality_counters\r\n    WHERE time >= NOW() - INTERVAL '10 minutes'\r\n    ORDER BY time DESC\r\n    LIMIT 1\r\n)\r\nSELECT \r\n    'Line OEE' as metric,\r\n    ROUND((b.bottleneck_oee * q.quality_percent / 100.0)::numeric, 1) as value\r\nFROM bottleneck b\r\nCROSS JOIN current_quality q;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Production Line OEE - Current Hour",
      "transparent": true,
      "type": "bargauge"
